<?xml version="1.0" encoding="UTF-8"?>
<ehcache 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
	updateCheck="false"
	>
		<defaultCache
			maxElementsInMemory="0"
			statistics="true"
			eternal="false"
			overflowToDisk="false"
			timeToLiveSeconds="600"/>
		<cache 
			maxElementsInMemory="0"
			statistics="true"
			name="itemList" 
			eternal="false"
			overflowToDisk="false" 
			timeToLiveSeconds="600"
		/>
		<cache 
			maxElementsInMemory="0"
			statistics="true"
			name="article" 
			eternal="false"
			overflowToDisk="false" 
			timeToLiveSeconds="600"
		/>
		<cache 
			maxElementsInMemory="0"
			statistics="true"
			name="activeArticle" 
			eternal="false"
			overflowToDisk="false" 
			timeToLiveSeconds="600"
		/>
		<cache 
			maxElementsInMemory="0"
			statistics="true"
			name="topItemList" 
			eternal="false"
			overflowToDisk="false" 
			timeToLiveSeconds="600"
		/>
</ehcache>